{
	"name": "elysia-react-router",
	"private": true,
	"sideEffects": false,
	"type": "module",
	"scripts": {
		"build": "react-router build",
		"dev": "bun --watch ./server/index.ts",
		"format": "prettier --ignore-path .gitignore --ignore-unknown --cache --cache-location node_modules/.cache/prettiercache --write .",
		"lint": "eslint --ignore-path .gitignore --no-error-on-unmatched-pattern --cache --cache-location node_modules/.cache/eslint --fix .",
		"start": "NODE_ENV=production bun ./server/index.ts"
	},
	"dependencies": {
		"@react-router/fs-routes": "^7.1.1",
		"@tailwindcss/vite": "^4.1.2",
		"elysia": "^1.2.10",
		"elysia-react-router": "^0.1.0",
		"isbot": "^5.1.21",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"react-router": "^7.1.1",
		"tailwindcss": "^4.1.2"
	},
	"devDependencies": {
		"@react-router/dev": "^7.1.1",
		"@remix-run/eslint-config": "^2.15.2",
		"@types/react": "^19.0.4",
		"@types/react-dom": "^19.0.2",
		"eslint": "^9.18.0",
		"prettier": "^3.4.2",
		"typescript": "^5.7.3",
		"vite": "^6.0.7",
		"vite-tsconfig-paths": "^5.1.4"
	}
}
